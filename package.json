{
  "name": "@mscharley/notes-nc",
  "productName": "notes-nc",
  "version": "1.0.0",
  "description": "Note taking app for markdown folders",
  "author": "Matthew Scharley <matt@scharley.me>",
  "license": "MIT",
  "main": "ts-build/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mscharley/notes-nc.git"
  },
  "bugs": {
    "url": "https://github.com/mscharley/notes-nc/issues"
  },
  "homepage": "https://github.com/mscharley/notes-nc#readme",
  "engines": {
    "node": ">= 16.13",
    "npm": ">= 8.0"
  },
  "engineStrict": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "electron": "ELECTRON_IS_DEV=0 electron ./",
    "electron:dev": "ELECTRON_IS_DEV=1 electron ./",
    "start": "concurrently -n tsc,npm,webpack -c 'green,yellow,blueBright' npm:start:tsc npm:start:watch npm:start:webpack",
    "start:tsc": "tsc --watch",
    "start:watch": "npm-watch",
    "start:webpack": "webpack --watch",
    "clean": "rimraf -rf build ts-build dist",
    "build": "node ./bin/build.mjs",
    "copy-files": "copyfiles -u 1 src/**/*.css ts-build/",
    "test": "npm run test:lint",
    "test:electronegativity": "electronegativity -i ./src -e $(electron --version)",
    "test:lint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "prepare": "husky install",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "watch": {
    "copy-files": {
      "patterns": [
        "src"
      ],
      "extensions": "css"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix",
    "*.{json,md}": "prettier -w"
  },
  "dependencies": {
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.2.1",
    "@mui/material": "^5.2.3",
    "@reduxjs/toolkit": "^1.7.0",
    "easymde": "^2.16.1",
    "electron-log": "^4.4.1",
    "inversify": "^6.0.1",
    "inversify-token": "^6.0.0",
    "lodash.debounce": "^4.0.8",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.21",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@mscharley/eslint-config": "^1.7.1",
    "@mscharley/prettier-config": "^1.2.1",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/uuid": "^8.3.3",
    "concurrently": "^6.4.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.5.1",
    "electron": "^16.0.4",
    "electron-builder": "^22.14.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-is-dev": "^2.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "install": "^0.13.0",
    "lint-staged": "^12.1.2",
    "npm-watch": "^0.11.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
